    <script type="text/javascript">
        var ErrorDiv = $("#errorBlock").find(".alert.alert-danger.display-none");
        var SuccessDiv = $("#errorBlock").find(".alert.alert-success.display-none");
        $('.dateFormat').datepicker({
            language: 'en',
            format: 'dd-M-yyyy',
            todayHighlight: true,
            autoclose: true
        });
        //$('.timepicker').timepicker({
        //    timeFormat: 'h:mm p',
        //    interval: 30,
        //    minTime: '6',
        //    maxTime: '11:00pm',
        //    defaultTime: '11',
        //    startTime: '06:00',
        //    dynamic: false,
        //    dropdown: true,
        //    scrollbar: true
        //});
        function FormSuccessError1(Message, Status, parrentID, ScrollDiv) {
            hideAlert();
            if (Status == "Error") {
                FormError(Message, Status, parrentID);
                ErrorDiv.show();
            }
            else if (Status == "Success") {
                FormError(Message, Status, parrentID);
                SuccessDiv.show();
            }
            ScrollToForm(ScrollDiv);
        }
        function ScrollToForm(container) {
            $("html, body").animate({ scrollTop: $('#' + container).offset().top }, "slow");
        }
        function ScrollToTop() {
            $('html, body').animate({ scrollTop: 0 }, "slow");
        }

        function hideAlert() {
            SuccessDiv.hide();
            ErrorDiv.hide();
        }
       
        $("#ddlStudentList").change(function () {
            alert('asd')
            var ID = $('option:selected', this).val();
            if (ID == '' || ID <= 0) {
                return false;
            }
            $.ajax({
                type: "POST",
                url: '/Post/BindStudent',
                data: JSON.stringify({ ID: ID, type: "PostStudent" }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: false,
                cache: false,
                success: function (data) {
                    $("#ddlStudentList").empty().append('<option selected="selected" value="0">Select</option>');
                    if (data.length > 0) {
                        $.each(data, function () {
                            $("#ddlStudentList").append($("<option/>").val(this.SR_ID).text(this.SR_StudentName));
                        });
                    }
                }
            });
        });
    </script>
