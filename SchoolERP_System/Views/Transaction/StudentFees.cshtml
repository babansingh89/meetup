@{
    ViewBag.Title = "Student Fee Collection";
    //Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}



<input type="hidden" id="hdId" value="0" />
<input type="hidden" id="hdPrintId" value="0" />

<div class="row" id="div_Scroll">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <span class="pull-left">Student Fee Collection</span>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel-body">
                        @Html.Partial("~/Views/Shared/SuccessError.cshtml")
                        <div class="row" style="display:none;">
                            <div class="col-md-2">
                                <div class="form-group">
                                    <input type="radio" name="RadioFees" id="Radio1" class="custom-radio" value="1">Admission
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <input type="radio" name="RadioFees" id="Radio2" class="custom-radio" value="2">Collection
                                </div>
                            </div>
                            @*<div class="col-md-2">
                                    <div class="form-group">
                                        <input type="checkbox" id="check3" class="" value="3">Send SMS
                                    </div>
                                </div>*@
                        </div>

                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Date:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control dateFormat" id="txtDate" autocomplete="off" readonly />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Reg.No:<span style="color:red;">*</span></label>
                                    <div class="input-group input-group-sm">
                                        <input type="text" class="form-control" id="txtRegNo" autocomplete="off" />
                                        <span class="input-group-btn">
                                            <button class="btn btn-primary" id="btnsearch" type="button"><i class="fa fa-search" aria-hidden="true"></i></button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Name:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtName" autocomplete="off" disabled />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Class:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtclassName" autocomplete="off" disabled />
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Roll No:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtrollno" autocomplete="off" disabled />
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Section:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtSectionName" autocomplete="off" disabled />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Stream:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtStreamName" autocomplete="off" disabled />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Ph.No:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtPhNo" autocomplete="off" disabled />
                                </div>
                            </div>
                        </div>

                        <table class="table table-striped table-bordered table-hover table-condensed" id="tblfee">
                            <thead>
                                <tr style="background-color: #d9d9d9;">
                                    <th>Fee Head</th>
                                    <th>Fee Amount</th>
                                    <th>Total Inst.</th>
                                    <th>Inst.No</th>
                                    <th>Due Date</th>
                                    <th>Inst.Amt</th>
                                    <th>Late Fine</th>
                                    <th>Pay.Amt</th>
                                    <th>Dis.Amt</th>
                                    <th>Due.Amt</th>
                                    <th>@*<input type="checkbox" id="chkAll" />*@Action</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>

                        <div class="row">
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Total Payable:<span style="color:red;"></span></label>
                                    <input type="text" class="form-control" id="txtTotalPayable" autocomplete="off" disabled />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Discount:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtDiscount" autocomplete="off" disabled />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Payment Amount:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtPaymentAmount" autocomplete="off" disabled />
                                </div>
                            </div>
                            <div class="col-md-3">
                                <div class="form-group">
                                    <label class="control-label">Due Amount:<span style="color:red;"></span></label>
                                    <input type="text" class="form-control" id="txtDueAmount" autocomplete="off" disabled />
                                </div>
                            </div>
                        </div>
                        <hr />
                        <div class="row">
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="control-label">Pay Mode:<span style="color:red;">*</span></label>
                                    <select class="form-control" id="ddlPayMode">
                                        <option value="-1">Select</option>
                                        <option value="cash">Cash</option>
                                        @*
                                            <option value="creditCard">CreditCard</option>
                                            <option value="debitCard">DebitCard</option>*@
                                        <option value="draft">Draft</option>
                                        <option value="cheque">Cheque</option>
                                        <option value="neft">Online</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="control-label">Bank Name:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtBankName" disabled autocomplete="off" />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="control-label">Branch Name:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control" id="txtBranchName" disabled autocomplete="off" />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="control-label">Draft/Cheuqe No:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control Numeric" id="txtCheuqeNo" disabled autocomplete="off" />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="control-label">DD Date/Pay Date:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control dateFormat" id="txtPayDate" autocomplete="off" />
                                </div>
                            </div>
                            <div class="col-md-2">
                                <div class="form-group">
                                    <label class="control-label">Amount:<span style="color:red;">*</span></label>
                                    <input type="text" class="form-control Numeric" id="txtAmount" autocomplete="off" />
                                </div>
                            </div>
                            <div class="col-md-1 pull-right">
                                <div class="form-group">
                                    <button class="btn btn-primary" id="btnAddPay">ADD</button>
                                </div>
                            </div>
                        </div>

                        <table class="table table-striped table-bordered table-hover table-condensed" id="tblPayDetails">
                            <thead>
                                <tr>
                                    <th>Pay Mode</th>
                                    <th>Bank Name</th>
                                    <th>BranchName</th>
                                    <th>Draft/CH.No</th>
                                    <th>DraftDate</th>
                                    <th>Amount</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>

                        <div class="col-md-12">
                            <div class="form-group text-right">
                                <input type="checkbox" class="custom-control-input" id="chkSMS">
                                <label class="custom-control-label" for="customControlInline">Send SMS</label>
                                <button class="btn btn-primary" id="btnSubmit">SUBMIT</button>
                                <button class="btn btn-success" id="btnSubmitPrint">SUBMIT & PRINT</button>
                                @*<button class="btn btn-warning" disabled id="">SUBMIT & SEND</button>*@
                                <button class="btn btn-danger" id="btncancel">CANCEL</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section body_scripts{
    <script type="text/javascript">
        var ErrorDiv = $("#errorBlock").find(".alert.alert-danger.display-none");
        var SuccessDiv = $("#errorBlock").find(".alert.alert-success.display-none");
        function hideAlert() {
            SuccessDiv.hide();
            ErrorDiv.hide();
        }
        function getParameterByName(name) {
            name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
            var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
                results = regex.exec(location.search);
            return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
        }
        $(document).on('click', 'input.chkRow', function () {
            var d = $(this).parents('tr').find('#expenses_sum').val();
            if (parseInt(d) == 0) {
                var late = 0;
                var pay = 0;
                var dis = 0;
                var due = 0;
                $('#tblfee > tbody > tr').each(function () {

                    var v4 = $(this).parents('tr').find('#expenses_sum').val();

                    if ($(this).find('td input[type=checkbox]').prop('checked') == true) {

                        var v0 = $(this).find('td:eq(5)').text();
                        var v1 = $(this).find('#expenses_sum1').val();
                        var v2 = $(this).find('#expenses_sum2').val();
                        var v3 = $(this).find('#expenses_sum3').val();
                        var v4 = $(this).find('#expenses_sum').val();

                        if (v1.length == 0)
                            v1 = 0;
                        if (v2.length == 0)
                            v2 = 0;
                        if (v3.length == 0)
                            v3 = 0;
                        if (v4.length == 0)
                            v4 = 0;

                        late = parseInt(late) + parseInt(v1) + parseInt(v0);
                        pay = parseInt(pay) + parseInt(v2);
                        dis = parseInt(dis) + parseInt(v3);
                        due = parseInt(due) + parseInt(v4);
                    }
                });
                //alert(late); alert(pay); alert(dis); alert(due);
                $('#txtTotalPayable').val(late);
                $('#txtDiscount').val(dis);
                $('#txtPaymentAmount').val(pay);
                $('#txtDueAmount').val(due);
            } else {
                $(this).parents('tr').find('.chkRow').prop("checked", false);
                alert("Due Amount Greater than Zero!");
            }

        });
        $(document).on('keyup', 'input.expenses', function () {

            var v1 = $(this).parents('tr').find('td:eq(5)').text();
            var v2 = $(this).parents('tr').find('#expenses_sum1').val();
            var v3 = $(this).parents('tr').find('#expenses_sum2').val();
            var v4 = $(this).parents('tr').find('#expenses_sum3').val();
            if (v1.length == 0)
                v1 = 0;
            if (v2.length == 0)
                v2 = 0;
            if (v3.length == 0)
                v3 = 0;
            if (v4.length == 0)
                v4 = 0;

            var v5 = parseInt(v1) + parseInt(v2);

            var v6 = parseInt(v3) + parseInt(v4);

            $expenseTotal = $(this).parents('tr').find('#expenses_sum');

            var v7 = parseInt(v5) - parseInt(v6);

            if (v7 < 0) {
                alert('Please Enter Correct Amount!');
                return;
            }
            else {
                $expenseTotal.val('0');
                $expenseTotal.val(v7);
            }

            //$expenses = $(this).parents('tr').find('.expenses');
            //$expenseTotal = $(this).parents('tr').find('#expenses_sum');
            //$expenseTotal.val('0');
            //$.each($expenses, function (index, object) {

            //    if ($(object).val() != '') {
            //        $expenseTotal.val(parseInt($expenseTotal.val()) + parseInt($(object).val()));
            //    }
            //});
            $(this).parents('tr').find('.chkRow').prop("checked", false);

        });
        $(document).ready(function () {
            var d = new Date();
            var currMonth = d.getMonth();
            var currYear = d.getFullYear();
            var day = d.getDate();

            var startDate = new Date(currYear, currMonth, day);

            $("#txtDate").datepicker("setDate", startDate);

            var query_event_id = getParameterByName('_Id');

            if (query_event_id) {
                $('#txtRegNo').val(query_event_id);
                $('#btnsearch').click();
            }
        });
        $(document).on('click', '#btnsearch', function () {
            hideAlert();
            var _RegNo = $('#txtRegNo').val();
            if (_RegNo == '') {
                FormSuccessError1("Please enter Reg No.", "Error", "errorBlock", "div_Scroll");
                $('#txtRegNo').focus();
                return false;
            }

            $.ajax({
                type: "POST",
                url: '@Url.Action("GetStudentFee", "Transaction")',
                data: JSON.stringify({ RegNo: _RegNo }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {

                    if (data.length > 0) {
                        $("#hdId").val(data[0][0].SR_ID);
                        $('#txtRegNo').val(data[0][0].SR_RegNo);
                        $('#txtName').val(data[0][0].SR_StudentName);
                        $('#txtclassName').val(data[0][0].ClassName);
                        $('#txtrollno').val(data[0][0].SR_CurrentRollNo);
                        $('#txtSectionName').val(data[0][0].SectionName);
                        $('#txtStreamName').val(data[0][0].StreamName);
                        $('#txtPhNo').val(data[0][0].SR_PhNo);
                        $('#tblfee tbody').empty();
                        $.each(data[1], function (rowIndex, val) {
                            var row = '';
                            row += '<tr id=' + val.FeeHeadID + '>';
                            row += '<td style="vertical-align: middle;">' + val.FeeHeadName + '</td>';
                            row += '<td style="vertical-align: middle;">' + val.Total_Fee_Amt + '</td>';
                            row += '<td style="vertical-align: middle;">' + val.Total_No_Ins + '</td>';
                            row += '<td>' + val.Ins_No + '</td>';
                            row += '<td>' + val.Due_Date + '</td>';
                            row += '<td>' + val.Ins_Amt + '</td>';
                            if (val.SFCT_DueAmount.length == 0) {
                                row += '<td><input type="textbox" id="expenses_sum1" class="form-control Numeric expenses" value=' + val.Late_Fee + ' ></td>';
                                row += '<td><input type="textbox" id="expenses_sum2" class="form-control Numeric expenses" value=' + val.SFCT_PaymentAmount + ' ></td>';
                                row += '<td><input type="textbox" id="expenses_sum3" class="form-control Numeric expenses" value=' + val.SFCT_DiscountAmount + ' ></td>';
                                row += '<td><input type="textbox" id="expenses_sum" class="form-control Numeric" disabled  value=' + val.SFCT_DueAmount + '></td>';
                                row += '<td><input type="checkbox" class="chkRow" id="chk" /></td>';
                            } else {
                                row += '<td><input type="textbox" id="expenses_sum1" class="form-control Numeric expenses" value=' + val.Late_Fee + ' disabled ></td>';
                                row += '<td><input type="textbox" id="expenses_sum2" class="form-control Numeric expenses" value=' + val.SFCT_PaymentAmount + ' disabled></td>';
                                row += '<td><input type="textbox" id="expenses_sum3" class="form-control Numeric expenses" value=' + val.SFCT_DiscountAmount + ' disabled></td>';
                                row += '<td><input type="textbox" id="expenses_sum" class="form-control Numeric" disabled  value=' + val.SFCT_DueAmount + ' disabled></td>';
                                row += '<td></td>';
                            }
                            row += '</tr>';
                            $('#tblfee tbody').append(row);
                        });
                        //$('#tblfee').dataTable({ 'rowsGroup': [0,1,2] });
                        // MergeGridCells();
                        if ($.fn.dataTable.isDataTable('#tblfee')) {
                            //table = $('#tblfee').DataTable();
                            table = $('#tblfee').DataTable({
                                'rowsGroup': [0, 1, 2],
                                paging: false,
                                "searching": false
                            });
                        }
                        else {
                            table = $('#tblfee').DataTable({
                                'rowsGroup': [0, 1, 2],
                                paging: false,
                                "searching": false
                            });
                        }
                        $('.dataTables_info').hide();
                    } else {
                        FormSuccessError1("Please enter correct Reg No.", "Error", "errorBlock", "div_Scroll");
                        $('#txtRegNo').focus();
                        return false;
                    }
                },
                error: function (error) {
                    console.error(error);
                    //FormSuccessError(error, "Error", "errorBlock");
                    return false;
                }
            });
        });
        $("#ddlPayMode").change(function () {
            var _PayMode = $('option:selected', this).val();

            if ((_PayMode != '-1') && _PayMode != 'cash') {
                $('#txtBankName').removeAttr("disabled");
                $('#txtBranchName').removeAttr("disabled");
                $('#txtCheuqeNo').removeAttr("disabled");
            } else {

                $('#txtBankName').attr("disabled", "disabled");
                $('#txtBranchName').attr("disabled", "disabled");
                $('#txtCheuqeNo').attr("disabled", "disabled");
            }
        });
        $(document).on('click', '#btnAddPay', function () {
            hideAlert();
            var _PayMode = $('#ddlPayMode :selected').val();
            var _BankName = $('#txtBankName').val();
            var _BranchName = $('#txtBranchName').val();
            var _CheuqeNo = $('#txtCheuqeNo').val();
            var _DraftDate = $('#txtPayDate').val();
            var _Amount = $('#txtAmount').val();

            //alert(_PayMode);
            if (_PayMode == '' || _PayMode <= 0) {
                FormSuccessError1("Please select Pay Mode.", "Error", "errorBlock", "div_Scroll");
                return false;
            }
            if (_BankName == '' && _PayMode != 'cash') {
                FormSuccessError1("Please enter Bank Name.", "Error", "errorBlock", "div_Scroll");
                $('#txtBankName').focus();
                return false;
            }
            if (_BranchName == '' && _PayMode != 'cash') {
                FormSuccessError1("Please enter Branch Name.", "Error", "errorBlock", "div_Scroll");
                $('#txtBranchName').focus();
                return false;
            }
            if (_CheuqeNo == '' && _PayMode != 'cash') {
                FormSuccessError1("Please enter Cheuqe No.", "Error", "errorBlock", "div_Scroll");
                $('#txtCheuqeNo').focus();
                return false;
            }
            if (_DraftDate == '') {
                FormSuccessError1("Please enter date.", "Error", "errorBlock", "div_Scroll");
                $('#txtPayDate').focus();
                return false;
            } if (_Amount == '') {
                FormSuccessError1("Please enter Amount.", "Error", "errorBlock", "div_Scroll");
                $('#txtAmount').focus();
                return false;
            }

            $('#tblPayDetails tbody').append('<tr><td>' + _PayMode + '</td><td>' + _BankName + '</td><td>' + _BranchName + '</td><td>' + _CheuqeNo + '</td><td>' + _DraftDate + '</td><td>' + _Amount + '</td><td><i class="fa fa-times fadel" aria-hidden="true"></i></td></tr>');
            fn_resetpay();
        });

        $(document).on('click', '.fadel', function () {
            $(this).closest("tr").remove();
        });
        $(document).on('click', '#btncancel', function () {
            var url = '@Url.Action("StudentFeesCollectionList", "Transaction")';
            window.location.href = url;
        });

        function fn_resetpay() {
            $('#ddlPayMode').val('-1');
            $('#txtBankName').val('');
            $('#txtBranchName').val('');
            $('#txtCheuqeNo').val('');
            $('#txtPayDate').val('');
            $('#txtAmount').val('');
        }
        $(document).on('click', '#btnSubmitPrint', function () {
            $('#hdPrintId').val('1');
            $("#btnSubmit").click();
        });
        function GeneratePrintPDF(StdId) {

            $.ajax({
                type: "POST",
                url: '@Url.Action("FeePrintPDF", "Transaction")',
                data: JSON.stringify({ StdId: StdId }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (data) {
                    if (data == "Error") {
                        alert("Error! Try again later.");
                    }
                    else {
                        window.open(data);
                        //previewPdfDocument(data);
                    }
                },
                error: function (error) {
                    console.log(error);
                    //alert("Error! Try again later.");
                    window.location.href = "/Error";
                }
            });
        }

        $(document).on('click', '#btnSubmit', function () {
            hideAlert();
            var isSMS = false;
            if ($('#chkSMS').is(":checked")) {
                isSMS = true;
            }

            var _SR_ID = $('#hdId').val();
            var _Date = $('#txtDate').val();
            var _TotalPayable = $('#txtTotalPayable').val();
            var _Discount = $('#txtDiscount').val();
            var _PaymentAmount = $('#txtPaymentAmount').val();
            var _DueAmount = $('#txtDueAmount').val();
            var _ph = $('#txtPhNo').val();
            if (_PaymentAmount == '') {
                FormSuccessError1("Please Payment Amount!", "Error", "errorBlock", "div_Scroll");
                return false
            }
            var DataFee = [];
            $('#tblfee > tbody > tr').each(function () {
                if ($(this).find('td input[type=checkbox]').prop('checked') == true) {
                    var v0 = this.id;
                    var v1 = $(this).find('td:eq(3)').text();
                    var v2 = $(this).find('td:eq(6)').find("#expenses_sum1").val();
                    var v3 = $(this).find('td:eq(7)').find("#expenses_sum2").val();
                    var v4 = $(this).find('td:eq(8)').find("#expenses_sum3").val();
                    var v5 = $(this).find('td:eq(9)').find("#expenses_sum").val();

                    if (v2.length == 0)
                        v2 = 0;
                    if (v3.length == 0)
                        v3 = 0;
                    if (v4.length == 0)
                        v4 = 0;
                    if (v5.length == 0)
                        v5 = 0;
                    DataFee.push({
                        'SFCT_FeeHeadID': v0, 'SFCT_InstallmentNo': v1, 'SFCT_LateFee': v2, 'SFCT_PaymentAmount': v3, 'SFCT_DiscountAmount': v4, 'SFCT_DueAmount': v5
                    });
                }
            });

            var Datapay = [];
            var amtpaid = 0;
            $('#tblPayDetails > tbody > tr').each(function () {
                var v0 = $(this).find('td:eq(0)').text();
                var v1 = $(this).find('td:eq(1)').text();
                var v2 = $(this).find('td:eq(2)').text();
                var v3 = $(this).find('td:eq(3)').text();
                var v4 = $(this).find('td:eq(4)').text();
                var v5 = $(this).find('td:eq(5)').text();
                amtpaid += parseFloat(v5);
                Datapay.push({
                    'Pay_Mode': v0, 'Bank_Name': v1, 'Branch_Name': v2, 'D_C_No': v3, 'Pay_Date': v4, 'Pay_Amount': v5
                });
            });

            if (DataFee.length == 0) {
                FormSuccessError1("Please enter Payment details!", "Error", "errorBlock", "div_Scroll");
                return false;
            }
            if (Datapay.length == 0) {
                FormSuccessError1("Please enter Payment Mode details!", "Error", "errorBlock", "div_Scroll");
                return false;
            }

            if (parseFloat(_PaymentAmount) != parseFloat(amtpaid)) {
                FormSuccessError1("Please enter correct Payment Mode details, " + (parseFloat(_PaymentAmount) - parseFloat(amtpaid)) + " more rquired!", "Error", "errorBlock", "div_Scroll");
                return false;
            }

            $.ajax({
                type: "POST",
                url: '@Url.Action("saveStudentFeesCollection", "Transaction")',
                data: JSON.stringify({ SR_ID: _SR_ID, Date: _Date, TotalPayable: _TotalPayable, Discount: _Discount, PaymentAmount: _PaymentAmount, DueAmount: _DueAmount, DataFee, Datapay, isSMS: isSMS,ph:_ph }),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                cache: false,
                success: function (response) {

                    if (response > 0) {
                        fn_resetpay();
                        $('#tblPayDetails tbody').empty();
                        $('#tblfee tbody').empty();
                        $('#hdId').val(0);
                        $('#txtDate').val('');
                        $('#txtTotalPayable').val('');
                        $('#txtDiscount').val('');
                        $('#txtPaymentAmount').val('');
                        $('#txtDueAmount').val('');
                        $('#txtRegNo').val('');
                        $('#txtName').val('');
                        $('#txtclassName').val('');
                        $('#txtrollno').val('');
                        $('#txtSectionName').val('');
                        $('#txtStreamName').val('');
                        $('#txtPhNo').val('');
                        FormSuccessError1("Data Inserted successfully.", "Success", "errorBlock", "div_Scroll");
                        if ($('#hdPrintId').val() == '1') {
                            GeneratePrintPDF(response);
                            $('#hdPrintId').val('0');
                        }
                    }
                    else {
                        $('#hdPrintId').val('0');
                        alert("Error. Try again latet.");
                        return false;
                    }
                },
                error: function (error) {
                    console.error(error);
                    FormSuccessError1(error, "Error", "errorBlock", "div_Scroll");
                    return false;
                }
            });

        });


    </script>
}